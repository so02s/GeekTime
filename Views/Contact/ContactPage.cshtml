@model IList<GeekTime.Models.Contact>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ContactPage</title>
</head>
<body>
    <h1>Контакты администрации GeekTime:</h1>
    <table border="1" width="100%" cellpadding="5">
        <tr>
            <th>Id</th>
            <th>Имя</th>
            <th>Номер телефона</th>
            <th>Электронная почта</th>
            <th>Должность</th>
        </tr>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.Name</td>
                    <td>@item.TelNumber</td>
                    <td>@item. Mail</td>
                    <td>@item.Post</td>
                </tr>
            }
        }
    </table>
    <a style="color:#ff0000" asp-area="" asp-controller="Contact" asp-action="CreateContact">Добавить контакт</a>
    <a style="color:#ff0000" asp-area="" asp-controller="Contact" asp-action="DeleteContact">Удалить контакт</a>
</body>
</html>
